<h1>Edit Build</h1>
<%= form_for ([@skill, @build]) do |f| %>

  <%= f.label :name %><br/>
  <%= f.text_field :name %>
  </br>
  <%= f.label :details %><br/>
  <%= f.text_area :details %>
  </br>
  <%= f.label :img_url %><br/>
  <%= f.text_field :img_url %>
  </br>
  <%= f.label :vid_url %><br/>
  <%= f.text_field :vid_url %>
  </br>
  <%= f.submit %>
<% end %>
