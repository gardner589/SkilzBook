<br/>
<div class="">
<h1><%= @build.name %></h1>
<img class="bld-img" src="<%= @build.img_url%>">
<p><%= @build.details %></p>
<div class="">
  <div class="">
    <h2 class="">Links</h2>
    <% if current_user %>
    <p><%= link_to '[+]', new_build_link_path(@build) %></p>
    <%end%>
  </div>
<% if @build.links.last %>
  <% @build.links.each do |link| %>
    <%= link_to @build.name + " skillz " + link.title, link.link_url %>
    <% if current_user %>
    <p><%= link_to "Edit", edit_build_link_path(@build, link)%></p>
    <p><%= link_to 'Delete', build_link_path(@build, link), method: :delete, data: { confirm: 'Are you sure?' } %></p>
    <%end%>
    <br>
  <%end%>
<%end%>
</div>
<!-- <%= button_to "Back", skill_path, class: "btn btn-success btn-sm glyphicon glyphicon-hand-left"%> -->
<a href="/skills/<%= @skill.id %>">
  <button type="button" class="btn btn-info btn-md glyphicon glyphicon-hand-left"> Back</button>
</a>
<br>
<div class="">
<h2 class="">Comments</h2>
<p><%= link_to '[+]', new_build_comment_path(@build) %></p>

</div>
  <div class="">
    <% if @build.comments.last %>
      <% @build.comments.each do |comment|%>
        <p class=""><%= comment.body %></p>
      <%end%>
    <%end%>
  </div>
</div>
